                                <td style="font-size:small">@AasContext.SerializeElement(cdDB.Administration)</td>
                                <td style="font-size:small">@AasContext.SerializeList(cdDB.IsCaseOf)</td>
                                <td style="font-size:small">@AasContext.SerializeList(cdDB.EmbeddedDataSpecifications)</td>
                                <td style="width:175px;color:lightgray;font-size:x-small">
                                    @($"CREATE {TimeStamp.DateTimeToString(cdDB.TimeStampCreate)}")<br />
                                    @($"UPDATE {TimeStamp.DateTimeToString(cdDB.TimeStamp)}")<br />
                                    @($"TREE {TimeStamp.DateTimeToString(cdDB.TimeStampTree)}")<br />
                                    @($"DELETE {TimeStamp.DateTimeToString(cdDB.TimeStampDelete)}")
                                </td>
                                <td style="word-break:keep-all">
