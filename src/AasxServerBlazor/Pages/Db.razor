                    <thead>
                    <tr>
                        <th style="word-break:keep-all">Tabel</th>
                        <th style="word-break:keep-all">Count</th>
                        <th style="word-break:keep-all">Links</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr>
                        <td>
                            <span>Env#:</span>
                        </td>
                        <td>
                            <span>@db.EnvSets.Count()</span>
                        </td>
                        <td>
                            <a href="@($"{url}/env")" target="_blank">env</a>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <span>CD#:</span>
                        </td>
