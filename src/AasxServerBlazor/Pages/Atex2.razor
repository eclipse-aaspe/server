            // double clMax2 = (0.5 * C[iOut] - C[iIn]) / 0.11;
            double clMax2 = (Co - C[iIn]) / 0.11;
            if (clMax2 < clMax1)
                clMax1 = clMax2;
            if (clMax1 < 0)
                clMax1 = 0;
